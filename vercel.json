{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/predict.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/get-sign-description.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict.js"
    },
    {
      "src": "/api/get-sign-description",
      "dest": "/api/get-sign-description.js"
    }
  ]
}
